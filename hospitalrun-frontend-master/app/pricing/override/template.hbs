{{#modal-dialog
    isUpdateDisabled=isUpdateDisabled
    title=title
    updateButtonAction=updateButtonAction
    updateButtonText=updateButtonText }}
  {{#em-form model=model submitButton=false}}
    {{em-select label=(t 'billing.labels.pricingProfile')
      property="profile"
      content=pricingProfiles
      optionValuePath="selectObject"
      optionLabelPath="selectObject.name"
      class="required pricing-profile"
      prompt=" "
    }}
    {{number-input label=(t 'labels.price') property="price" class="required pricing-override-price"}}
  {{/em-form}}
{{/modal-dialog}}
