{{#modal-dialog
    hideCancelButton=hideCancelButton
    hideUpdateButton=hideUpdateButton
    isUpdateDisabled=isUpdateDisabled
    title=title
    updateButtonAction=updateButtonAction
    updateButtonText=updateButtonText }}
  {{#em-form model=model submitButton=false }}
    {{em-select label=(t 'inventory.reports.rows.category') property="category" class="required"
      content=categoryTypes
      prompt=" "
    }}
    {{em-input label=(t 'patients.labels.sources') property="sources"}}
    {{number-input label=(t 'patients.labels.cost') property="cost" class="required"}}
  {{/em-form}}
{{/modal-dialog}}
